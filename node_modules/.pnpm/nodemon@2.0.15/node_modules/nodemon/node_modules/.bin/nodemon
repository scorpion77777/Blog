#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules:/Users/varhanmartirosyan/Desktop/node_modules:/Users/varhanmartirosyan/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/nodemon@2.0.15/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules/.pnpm/node_modules:/Users/varhanmartirosyan/Desktop/Blog/node_modules:/Users/varhanmartirosyan/Desktop/node_modules:/Users/varhanmartirosyan/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
